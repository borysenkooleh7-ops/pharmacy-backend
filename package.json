{
  "name": "apoteka24-backend",
  "version": "1.0.0",
  "description": "Backend API for Apoteka24.me - Montenegro pharmacy aggregator",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "dev:watch": "NODE_ENV=development nodemon index.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "migration:create": "npx sequelize-cli migration:generate --name",
    "seeder:create": "npx sequelize-cli seed:generate --name",
    "seed": "node seed.js",
    "seed:dev": "NODE_ENV=development node seed.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "pharmacy",
    "montenegro",
    "api",
    "express",
    "postgresql",
    "healthcare"
  ],
  "author": "Apoteka24.me Team",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "pg": "^8.16.3",
    "sequelize": "^6.35.2",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
